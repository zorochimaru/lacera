{
  "name": "lacera",
  "version": "0.0.66",
  "scripts": {
    "start": "npm run make-sprite && ng serve -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "format": "prettier --write .",
    "lint-fix": "ng lint --fix",
    "stylelint": "stylelint '**/*{.css,.scss}'",
    "stylelint:fix": "stylelint '**/*{.css,.scss}' --fix",
    "make-sprite": "svg-sprite -C svg-sprite.json src/assets/icons/svg/**/*.svg",
    "i18n:extract": "transloco-keys-manager extract -u --sort",
    "i18n:find": "transloco-keys-manager find --translations-path assets/i18n",
    "transloco:optimize": "transloco-optimize dist/lacera/browser/assets/i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.2",
    "@angular/cdk": "^20.2.1",
    "@angular/common": "^20.2.2",
    "@angular/compiler": "^20.2.2",
    "@angular/core": "^20.2.2",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.2.2",
    "@angular/platform-browser": "^20.2.2",
    "@angular/platform-browser-dynamic": "^20.2.2",
    "@angular/router": "^20.2.2",
    "@jsverse/transloco": "^7.6.1",
    "@jsverse/transloco-messageformat": "^7.0.1",
    "@jsverse/transloco-optimize": "^7.0.1",
    "@jsverse/transloco-persist-lang": "^7.0.2",
    "@types/lodash-es": "^4.17.12",
    "date-fns": "^4.1.0",
    "firebase-admin": "^13.4.0",
    "lodash-es": "^4.17.21",
    "ngx-mask": "^20.0.3",
    "pica": "^9.0.1",
    "rxjs": "~7.8.0",
    "swiper": "^11.2.10",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^20.2.0",
    "@angular-eslint/eslint-plugin": "^20.2.0",
    "@angular-eslint/eslint-plugin-template": "^20.2.0",
    "@angular-eslint/schematics": "^20.2.0",
    "@angular-eslint/template-parser": "^20.2.0",
    "@angular/build": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.2",
    "@jsverse/transloco-keys-manager": "^4.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/pica": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "firebase-tools": "^14.15.0",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.1.0",
    "svg-sprite": "^2.0.2",
    "typescript": "~5.8.3"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.scss": "stylelint --fix"
  }
}
