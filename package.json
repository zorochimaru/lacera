{
  "name": "lacera",
  "version": "0.0.21",
  "scripts": {
    "start": "ng serve -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "stylelint": "stylelint '**/*{.css,.scss}'",
    "stylelint:fix": "stylelint '**/*{.css,.scss}' --fix",
    "make-sprite": "svg-sprite -C svg-sprite.json ./web/lacera-app/src/assets/icons/svg/./*.svg",
    "i18n:extract": "transloco-keys-manager extract -u --sort",
    "i18n:find": "transloco-keys-manager find --translations-path assets/i18n",
    "transloco:optimize": "transloco-optimize dist/lacera/browser/assets/i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@jsverse/transloco": "^7.3.0",
    "@jsverse/transloco-messageformat": "^7.0.0",
    "@jsverse/transloco-optimize": "^7.0.0",
    "@jsverse/transloco-persist-lang": "^7.0.0",
    "@types/lodash-es": "^4.17.12",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.1",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "^17.2.1",
    "@angular-eslint/eslint-plugin-template": "^17.2.1",
    "@angular-eslint/schematics": "^17.2.1",
    "@angular-eslint/template-parser": "^17.2.1",
    "@angular/cli": "^17.2.1",
    "@angular/compiler-cli": "^17.2.0",
    "@jsverse/transloco-keys-manager": "^4.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "firebase-tools": "^13.7.5",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.2",
    "ngx-build-plus": "^17.0.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.1.0",
    "svg-sprite": "^2.0.2",
    "typescript": "~5.3.2"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.scss": "stylelint --fix"
  }
}
